import React, { Component } from "react";
import { Redirect } from "react-router";
import { Switch } from "react-router-dom";

import PrivateRoute from "@routes/privateRoute";
import Home from "@pages/home";
import Device from "@pages/device";
import LookStudent from "@pages/student/look";
import EditStudent from "@pages/student/edit";

class Index extends Component {

    render() {
        return (
            <Switch>
                <PrivateRoute exact path="/home" component={Home} />
                <PrivateRoute exact path="/device" component={Device} />
                <PrivateRoute exact path="/student/look" component={LookStudent} />
                <PrivateRoute exact path="/student/edit" component={EditStudent} />
                <Redirect exact from="" to="/home" />
            </Switch>
        );
    }
}

export default Index;
